<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CarteHs" table="carte_hs">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idVersionNomenclature" type="integer" column="id_version_nomenclature" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idLot" type="integer" column="id_lot" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numSerie" type="string" column="num_serie" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
