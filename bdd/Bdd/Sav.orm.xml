<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\ProduitFiniBundle\Entity\Sav" table="sav">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numSerie" type="string" column="num_serie" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="etape" type="integer" column="etape" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etat" type="integer" column="etat" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateReception" type="date" column="date_reception" nullable="false"/>
    <field name="dateRenvoi" type="date" column="date_renvoi" nullable="false"/>
    <field name="commentaire" type="text" column="commentaire" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
