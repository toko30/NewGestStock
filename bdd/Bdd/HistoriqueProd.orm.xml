<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\AffichageBundle\Entity\HistoriqueProd" repository-class="IC\AffichageBundle\Repository\HistoriqueProdRepository" table="historique_prod">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idNomenclature" type="integer" column="id_nomenclature" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateProd" type="date" column="date_prod" nullable="false"/>
    <field name="lieu" type="integer" column="lieu" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
