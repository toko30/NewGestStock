<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Reprise" table="reprise">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idLot" type="integer" column="id_lot" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etape" type="integer" column="etape" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numSerie" type="string" column="num_serie" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numSerieProduitFini" type="string" column="num_serie_produit_fini" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="commentaire" type="text" column="commentaire" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="reprise" type="integer" column="reprise" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateTest" type="date" column="date_test" nullable="false"/>
  </entity>
</doctrine-mapping>
