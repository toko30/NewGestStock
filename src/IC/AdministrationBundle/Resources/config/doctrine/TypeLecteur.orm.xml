<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\AdministrationBundle\Entity\TypeLecteur" repository-class="IC\AdministrationBundle\Repository\TypeLecteurRepository" table="type_lecteur">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idNomenclature" type="integer" column="id_nomenclature" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="designation" type="text" column="designation" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="frequence" type="integer" column="frequence" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="petite" type="integer" column="petite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="moyenne" type="integer" column="moyenne" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="grande" type="integer" column="grande" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="nomenclature" target-entity="Nomenclature" inversed-by="typeLecteur">
      <join-column name="id_nomenclature" referenced-column-name="id" />
    </one-to-one>
    <one-to-one field="sousTypeLecteur" target-entity="SousTypeLecteur">
      <join-column name="type" referenced-column-name="id" />
    </one-to-one>
  </entity>
</doctrine-mapping>
