<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\AffichageBundle\Entity\Moq" repository-class="IC\AffichageBundle\Repository\MoqRepository" table="moq">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idComposant" type="integer" column="id_composant" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idFournisseur" type="integer" column="id_fournisseur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ref" type="text" column="ref" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="moq" type="integer" column="moq" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="fournisseur" target-entity="Fournisseur" >
      <join-column name="id_fournisseur" referenced-column-name="id" />
    </one-to-one>
    <one-to-one field="composant" target-entity="Composant" >
      <join-column name="id_composant" referenced-column-name="id" />
    </one-to-one>
  </entity>
</doctrine-mapping>
