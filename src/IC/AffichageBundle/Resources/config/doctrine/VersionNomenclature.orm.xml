<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\AffichageBundle\Entity\VersionNomenclature" repository-class="IC\AffichageBundle\Repository\VersionNomenclatureRepository" table="version_nomenclature">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idNomenclature" type="integer" column="id_nomenclature" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="version" type="integer" column="version" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <one-to-one field="nomenclature" target-entity="Nomenclature" >
      <join-column name="id_nomenclature" referenced-column-name="id" />
    </one-to-one>
  </entity>
</doctrine-mapping>
