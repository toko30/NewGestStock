{% extends "ICAffichageBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - Identifiant{% endblock %}

{% block entete %}{{ parent() }} Identifiant{% endblock %}

{% block stylesheets %}
	{% stylesheets filter='cssrewrite'
	'@ICAffichageBundle/Resources/public/css/sous_traitant.css' %}
		{{ parent() }}
		<link rel="stylesheet" href="{{ asset_url }}" type="text/css">
	{% endstylesheets %}
{% endblock %}

{% block page %}
{{ parent() }}
	<h1>Stock Identifiant</h1>
	<table class="imp">
		<tr>
			<td class="titreReference">Référence</td>
			<td class="titreDesignation">Désignation</td>
			<td class="titreStock">Quantité</td>
		</tr>
		{% for badge in badges %}
			<tr>
				<td>{{badge.reference}}</td>
				<td class="designation">{{badge.designation}}</td>
				<td>{{badge.quantite}}</td>
			</tr>
		{% endfor %}
	</table>
{% endblock %}
