<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\ProduitFiniBundle\Entity\ProduitFiniAutre" repository-class="IC\ProduitFiniBundle\Repository\ProduitFiniAutreRepository" table="produit_fini_autre">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idAutre" type="integer" column="id_autre" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="numSerie" type="string" column="num_serie" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="vendu" type="integer" column="vendu" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAjout" type="date" column="date_ajout" nullable="false"/>
    <one-to-one field="autre" target-entity="Autre" >
      <join-column name="id_autre" referenced-column-name="id" />
    </one-to-one>
  </entity>
</doctrine-mapping>
