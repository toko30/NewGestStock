<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\ProduitFiniBundle\Entity\Etape" repository-class="IC\ProduitFiniBundle\Repository\EtapeRepository" table="etape">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="text" column="nom" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <one-to-many field="test" target-entity="Test" mapped-by="etape"/>
  </entity>
</doctrine-mapping>
