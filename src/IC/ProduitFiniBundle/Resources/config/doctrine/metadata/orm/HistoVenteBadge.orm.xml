<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HistoVenteBadge" table="histo_vente_badge">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idClient" type="integer" column="id_client" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idTypeBadge" type="integer" column="id_type_badge" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateVente" type="date" column="date_vente" nullable="true"/>
  </entity>
</doctrine-mapping>
