<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CarteTest" table="carte_test">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numSerie" type="string" column="num_serie" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idLot" type="integer" column="id_lot" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="etat" type="boolean" column="etat" nullable="false"/>
    <field name="assemble" type="boolean" column="assemble" nullable="false"/>
  </entity>
</doctrine-mapping>
