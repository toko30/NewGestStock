<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HistoAppro" table="histo_appro">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idFournisseur" type="integer" column="id_fournisseur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idProduit" type="integer" column="id_produit" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="typeProduit" type="integer" column="type_produit" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateAppro" type="date" column="date_appro" nullable="false"/>
  </entity>
</doctrine-mapping>
