<?php

namespace IC\TestBundle\Entity;

/**
 * Reprise
 */
class Reprise
{
    /**
     * @var integer
     */
    private $idLot;

    /**
     * @var string
     */
    private $numSerie;

    /**
     * @var string
     */
    private $numSerieProduitFini;

    /**
     * @var integer
     */
    private $etape;

    /**
     * @var integer
     */
    private $reprise;

    /**
     * @var boolean
     */
    private $assemble;

    /**
     * @var integer
     */
    private $idFirmware;

    /**
     * @var integer
     */
    private $id;

    /**
     * @var \IC\TestBundle\Entity\Lot
     */
    private $lot;


    /**
     * Set idLot
     *
     * @param integer $idLot
     *
     * @return Reprise
     */
    public function setIdLot($idLot)
    {
        $this->idLot = $idLot;

        return $this;
    }

    /**
     * Get idLot
     *
     * @return integer
     */
    public function getIdLot()
    {
        return $this->idLot;
    }

    /**
     * Set numSerie
     *
     * @param string $numSerie
     *
     * @return Reprise
     */
    public function setNumSerie($numSerie)
    {
        $this->numSerie = $numSerie;

        return $this;
    }

    /**
     * Get numSerie
     *
     * @return string
     */
    public function getNumSerie()
    {
        return $this->numSerie;
    }

    /**
     * Set numSerieProduitFini
     *
     * @param string $numSerieProduitFini
     *
     * @return Reprise
     */
    public function setNumSerieProduitFini($numSerieProduitFini)
    {
        $this->numSerieProduitFini = $numSerieProduitFini;

        return $this;
    }

    /**
     * Get numSerieProduitFini
     *
     * @return string
     */
    public function getNumSerieProduitFini()
    {
        return $this->numSerieProduitFini;
    }

    /**
     * Set etape
     *
     * @param integer $etape
     *
     * @return Reprise
     */
    public function setEtape($etape)
    {
        $this->etape = $etape;

        return $this;
    }

    /**
     * Get etape
     *
     * @return integer
     */
    public function getEtape()
    {
        return $this->etape;
    }

    /**
     * Set reprise
     *
     * @param integer $reprise
     *
     * @return Reprise
     */
    public function setReprise($reprise)
    {
        $this->reprise = $reprise;

        return $this;
    }

    /**
     * Get reprise
     *
     * @return integer
     */
    public function getReprise()
    {
        return $this->reprise;
    }

    /**
     * Set assemble
     *
     * @param boolean $assemble
     *
     * @return Reprise
     */
    public function setAssemble($assemble)
    {
        $this->assemble = $assemble;

        return $this;
    }

    /**
     * Get assemble
     *
     * @return boolean
     */
    public function getAssemble()
    {
        return $this->assemble;
    }

    /**
     * Set idFirmware
     *
     * @param integer $idFirmware
     *
     * @return Reprise
     */
    public function setIdFirmware($idFirmware)
    {
        $this->idFirmware = $idFirmware;

        return $this;
    }

    /**
     * Get idFirmware
     *
     * @return integer
     */
    public function getIdFirmware()
    {
        return $this->idFirmware;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set lot
     *
     * @param \IC\TestBundle\Entity\Lot $lot
     *
     * @return Reprise
     */
    public function setLot(\IC\TestBundle\Entity\Lot $lot = null)
    {
        $this->lot = $lot;

        return $this;
    }

    /**
     * Get lot
     *
     * @return \IC\TestBundle\Entity\Lot
     */
    public function getLot()
    {
        return $this->lot;
    }
}
