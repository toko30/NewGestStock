<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\ProductionBundle\Entity\OptionProduitFini" repository-class="IC\ProductionBundle\Repository\OptionProduitFiniRepository" table="option_produit_fini">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="abreviation" type="string" column="abreviation" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>

  </entity>
</doctrine-mapping>
