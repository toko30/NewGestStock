<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="IC\ProductionBundle\Entity\TypeLecteur" repository-class="IC\ProductionBundle\Repository\TypeLecteurRepository" table="type_lecteur">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="referenceInterne" type="string" column="reference_interne" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="referenceFournisseur" type="string" column="reference_fournisseur" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="designation" type="text" column="designation" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="integer" column="type" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="frequence" type="integer" column="frequence" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idFournisseur" type="integer" column="id_fournisseur" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="petite" type="integer" column="petite" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="moyenne" type="integer" column="moyenne" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="grande" type="integer" column="grande" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
